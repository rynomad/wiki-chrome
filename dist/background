},{"./_stream_duplex":86,"buffer":206,"core-util-is":91,"dup":20,"events":351,"inherits":85,"process-nextick-args":93,"util-deprecate":95}],91:[function(require,module,exports){
arguments[4][10][0].apply(exports,arguments)
},{"buffer":206,"dup":10}],92:[function(require,module,exports){
